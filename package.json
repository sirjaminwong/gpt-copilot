{
  "name": "gpt-copilot",
  "displayName": "Gpt copilot",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "shaojiang.wang",
  "scripts": {
    "dev": "plasmo dev --verbose",
    "build": "plasmo build",
    "package": "plasmo package",
    "lint:fix": "eslint . --ext '.js,.cjs,.jsx,.ts,.tsx' --fix",
    "lint": "eslint . '.js,.cjs,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@plasmohq/messaging": "^0.2.1",
    "@plasmohq/storage": "^1.7.2",
    "@trpc/client": "^10.37.1",
    "@trpc/next": "^10.37.1",
    "ahooks": "^3.7.8",
    "antd": "^5.8.3",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "hook": "link:@plasmohq/storage/hook",
    "md5": "^2.3.0",
    "openai": "^3.3.0",
    "plasmo": "0.74.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^1.13.1"
  },
  "devDependencies": {
    "@iconify/react": "^4.1.1",
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.227",
    "@types/node": "18.15.11",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.28",
    "prettier": "2.8.7",
    "tailwindcss": "^3.3.3",
    "typescript": "5.0.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "https://api.openai.com/*"
    ],
    "permissions": [
      "activeTab",
      "scripting",
      "commands",
      "bookmarks",
      "storage",
      "tabs",
      "webNavigation",
      "webRequest"
    ]
  }
}
